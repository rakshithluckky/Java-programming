
public class A {

    public static void main(String[] args) {
        B b = new B("Rakshith",19);
        System.out.println("Name: "+b.name+" "+"id:"+b.id);
    }
    
}
public class B {
    String name;
    int id;
    B(String name,int id){
        System.out.println("Parameterized Constructor in Class B");
        this.name=name;
        this.id=id;
    }
}
